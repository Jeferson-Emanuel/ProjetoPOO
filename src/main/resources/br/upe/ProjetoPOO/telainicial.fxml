<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" minHeight="480.0" minWidth="640.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.upe.ProjetoPOO.TelaInicialController">
   <children>
      <ImageView fitHeight="150.0" fitWidth="400.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../../../../../_UPE%202020.1(2021)/POO/Pictures/modern-apartments.jpg" />
         </image>
      </ImageView>
      <Button fx:id="controleApartamentos" alignment="CENTER" onAction="#switchToControleApartamentos" text="Controle de Apartamentos" />
      <Button fx:id="controleMorador" mnemonicParsing="false" onAction="#switchToControleMorador" text="Controle de Morador" />
      <Button fx:id="controleVeiculos" mnemonicParsing="false" onAction="#switchToControleVeiculo" text="Controle de Veículos" />
      <Button fx:id="controleFuncionario" mnemonicParsing="false" onAction="#switchToControleFuncionario" text="Controle de Funcionário" />
      <Button fx:id="almoxarifado" mnemonicParsing="false" onAction="#switchToAlmoxarifado" text="Almoxarifado" />
      <Button fx:id="controleReservas" mnemonicParsing="false" onAction="#switchToControleReservas" text="Controle de Reservas" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
